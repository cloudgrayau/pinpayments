    "keywords": [
      "pinpayments",
      "commerce",
      "craftcms",
      "omnipay",
      "pin"
    ],
    "support": {
        "docs": "https://github.com/cloudgrayau/pinpayments/blob/main/README.md",
        "issues": "https://github.com/cloudgrayau/pinpayments/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Cloud Gray Pty Ltd",
            "homepage": "https://cloudgray.com.au/"
        }
